pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('BUILD') {
            steps {
                echo 'DEVELOPMENT'
            }
        }
        stage('TEST') {
            steps {
                echo 'TESTING'
            }
        }
        stage('DEPLOY') {
            steps {
                echo 'MISE EN PRODUCTION'
            }
        }
    }
}
